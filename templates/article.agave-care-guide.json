            "text_wxNp38": {
              "type": "text",
              "name": "t:names.heading",
              "settings": {
                "text": "<h2>Soil Mix and Feeding</h2>",
                "width": "fit-content",
                "max_width": "normal",
                "max_width_tablet": "inherit",
                "max_width_desktop": "inherit",
                "alignment": "left",
                "type_preset": "rte",
                "font": "var(--font-body--family)",
                "font_size": "1rem",
                "line_height": "normal",
                "letter_spacing": "normal",
                "case": "none",
                "wrap": "pretty",
                "color": "var(--color-foreground)",
                "color_scheme": "",
