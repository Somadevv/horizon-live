            "text_b66tdk": {
              "type": "text",
              "name": "t:names.heading",
              "settings": {
                "text": "<h3>Growing <em>x Semponiums</em></h3>",
                "width": "fit-content",
                "max_width": "normal",
                "max_width_tablet": "inherit",
                "max_width_desktop": "inherit",
                "alignment": "left",
                "type_preset": "h2",
                "font": "var(--font-body--family)",
                "font_size": "1rem",
                "line_height": "normal",
                "letter_spacing": "normal",
                "case": "none",
