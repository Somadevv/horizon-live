                "text": "<h2>Mistake 2: Inadequate Light</h2>",
                "width": "fit-content",
                "max_width": "normal",
                "max_width_tablet": "inherit",
                "max_width_desktop": "inherit",
                "alignment": "left",
                "type_preset": "h2",
                "font": "var(--font-body--family)",
                "font_size": "1rem",
                "line_height": "normal",
                "letter_spacing": "normal",
