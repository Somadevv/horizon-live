    "section_winter_care": {
      "type": "section",
      "blocks": {
        "winter_heading": {
          "type": "text",
          "settings": {
            "text": "<h2>Winter Care Advice</h2>",
            "width": "100%",
            "max_width": "normal",
            "alignment": "left",
            "type_preset": "h2",
            "font": "var(--font-primary--family)",
            "font_size": "",
            "line_height": "normal",
            "letter_spacing": "normal",
            "case": "none",
