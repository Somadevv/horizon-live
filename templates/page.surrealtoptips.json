            "best_beginner_succulents": {
              "type": "_accordion-row",
              "settings": {
                "heading": "What are the best beginner succulents?"
              },
              "blocks": {
                "text_beginner_plants": {
                  "type": "text",
                  "settings": {
                    "text": "<p>We recommend starting with hardy varieties that are forgiving and easy to care for.</p>",
                    "type_preset": "rte",
                    "font": "var(--font-body--family)",
                    "font_size": "",
                    "line_height": "normal",
